msgid "[x-translation]"
msgstr ""

#: JmolGuide.docbook.xml:279(para) 
msgid "is the distance offset along x -- (integer)"
msgstr ""

#: JmolGuide.docbook.xml:279(option) 
msgid "[y-translation]"
msgstr ""

#: JmolGuide.docbook.xml:279(para) 
msgid "is the distance offset along y -- (integer)"
msgstr ""

#: JmolGuide.docbook.xml:279(option) 
msgid "[z-translation]"
msgstr ""

#: JmolGuide.docbook.xml:279(para) 
